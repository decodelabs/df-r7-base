{
    "name": "df-r7/base",
    "description": "DF r7 base",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Tom Wright",
        "email": "tom@inflatablecookie.com"
    }],
    "require": {
        "php": "^7.2|^8.0",
        "symfony/polyfill-mbstring": "~1.7",

        "decodelabs/exceptional": "^0.3",
        "decodelabs/glitch": "^0.17",
        "decodelabs/veneer": "^0.7",
        "decodelabs/systemic": "^0.7",
        "decodelabs/elementary": "^0.1",
        "decodelabs/tagged": "^0.10.1",
        "decodelabs/terminus": "^0.6",
        "decodelabs/collections": "^0.6",
        "decodelabs/gadgets": "^0.7",
        "decodelabs/atlas": "^0.8",
        "decodelabs/eventful": "^0.1",

        "guzzlehttp/guzzle": "^7.2",
        "erusev/parsedown": "^1.7",
        "nesbot/carbon": "^2.26"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "phpstan/phpstan": "^0.12.45",
        "phpstan/extension-installer": "^1.0",

        "decodelabs/phpstan-decodelabs": "^0.4",
        "decodelabs/chirp": "^0.6",

        "auth0/auth0-php": "^7.6",
        "stripe/stripe-php": "^6.38",
        "xenolope/quahog": "^2.1"
    },
    "autoload-dev": {
        "psr-4": {
            "DecodeLabs\\PHPStan\\": "phpstan/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
